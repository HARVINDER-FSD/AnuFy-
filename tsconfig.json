{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@/sdk": ["./src/sdk"],
      "@/services": ["./src/services"],
      "@/services/*": ["./src/services/*"],
      "@/hooks": ["./src/hooks"],
      "@/core/*": ["./src/core/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/**/*.ts", "src/**/*.tsx"],
  "exclude": [
    "node_modules",
    "routes/**/*",
    "services/**/*",
    "workers/**/*",
    "scripts/**/*",
    "app_v0modified.tsx",
    "api-server/**/*",
    "master/**/*"
  ]
}
